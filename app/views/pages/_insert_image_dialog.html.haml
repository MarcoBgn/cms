.js-dialog-target.is-inactive{ 'data-dough-dialog-target' => 'insert-image' }
  %div{ 'data-dough-component' => 'LinkManager', 'data-dough-insertmanager-target' => 'insert-image', 'data-dough-insertmanager-context' => 'insert-image' }
    .l-tab-panels.l-tab-panels--add-link
      .l-tab-panels__item.l-tab-panels__item--aside
        %h2.l-tab-panels__heading Add a image from...&hellip;
        .tab-selector__triggers-outer{'data-dough-tab-selector-triggers-outer' => true }
          .tab-selector__triggers-inner{'data-dough-tab-selector-triggers-inner' => true }
            .tab-selector__trigger-container.is-active{'data-dough-tab-selector-trigger-container' => true }
              %a.tab-selector__trigger{ href: '#library', 'data-dough-tab-selector-trigger' => 'library' } From library

      .l-tab-panels__item.l-tab-panels__item--content
        .tab-selector__target#file.is-inactive{ 'data-dough-tab-selector-target' => 'library' }
        %h3.l-tab-panels__heading From Library
        = render 'links/images/form'
