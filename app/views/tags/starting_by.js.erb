<% if @tags.any? %>
  $(".js-tags-existing-header").html("<%= escape_javascript(t('.header', prefix: @prefix)) %>");
  $(".js-tags-existing-header").show();
  $(".js-tags-existing").html("<%= escape_javascript(render(@tags)) %>");
<% else %>
  $(".js-tags-existing-header").hide();
  $(".js-tags-existing").html("<%= escape_javascript(content_tag(:span, t('.no_tags', prefix: @prefix))) %>");
<% end %>
